@using SpotifyVPY322.Dto.ArtistVM
@using SpotifyVPY322.Model

@model CreateArtistVM

<form asp-action="Create">
    <div>
        <label>Title: </label>
        <input asp-for="Title">
    </div>
    <div>
        <label>PhotoUrl: </label>
        <input asp-for="PhotoUrl">
    </div>
    <div>
        <label>Genre: </label>
        <select asp-for="Genre" asp-items="Model.Genres"></select> 
    </div>
    <input type="submit" value="Добавить"/>
</form>